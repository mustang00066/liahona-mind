{
  "name": "liahona-mind",
  "version": "1.0.0",
  "description": "LDS Hypnotherapy App for Breaking Habits - Your Spiritual Compass for Mental Peace",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "cap:init": "cap init \"Liahona Mind\" com.liahona.mind --web-dir dist",
    "cap:add": "cap add android",
    "cap:sync": "cap sync",
    "cap:open": "cap open android",
    "android": "npm run build && cap sync && cap open android",
    "android:build": "npm run build && cap sync && cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
    "@capacitor-community/text-to-speech": "^3.0.0",
    "date-fns": "^2.30.0",
    "localforage": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/cli": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/splash-screen": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.0"
  }
}
